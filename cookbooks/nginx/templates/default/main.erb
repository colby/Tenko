server {
    listen 80 default_server;
    listen [::]:80 default_server;

    return 301 https://$host$request_uri;
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;

    include conf.d/security;
    include conf.d/letsencrypt;

    server_name colbyolson.com www.colbyolson.com;

    include conf.d/provision;
    include conf.d/colbyolsoncom;
    include conf.d/printerfacts;
}
